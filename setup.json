{
    "name": "aiida-sssp-workflow",
    "author": "The AiiDA Team",
    "author_email": "morty.yeu@gmail.com",
    "description": "SSSP verification workflows",
    "url": "https://github.com/aiidateam/aiida-sssp-workflow",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.data": [
            "sssp_workflow = aiida_sssp_workflow.data:DiffParameters"
        ],
        "aiida.calculations": [
            "sssp_workflow = aiida_sssp_workflow.calculations.calculations:DiffCalculation",
            "sssp_workflow.birch_murnaghan_fit = aiida_sssp_workflow.calculations.birch_murnaghan_fit:birch_murnaghan_fit",
            "sssp_workflow.calculate_delta = aiida_sssp_workflow.calculations.calculate_delta:calculate_delta"
        ],
        "aiida.parsers": [
            "sssp_workflow = aiida_sssp_workflow.parsers:DiffParser"
        ],
        "aiida.cmdline.data": [
            "sssp_workflow = aiida_sssp_workflow.cli:data_cli"
        ],
        "aiida.workflows": [
            "sssp_workflow.eos = aiida_sssp_workflow.workflows.eos:EquationOfStateWorkChain"
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=1.1.0,<2.0.0",
        "six",
        "voluptuous"
    ],
    "extras_require": {
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=3.6,<5.0.0",
            "pytest-cov"
        ],
        "pre-commit": [
            "astroid==2.2.5",
            "pre-commit==1.20.0",
            "prospector==1.1.7",
            "pylint==2.3.1"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme"
        ]
    }
}
